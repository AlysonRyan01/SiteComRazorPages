@page "/clientes"
@model SiteComRazorPages.Pages.Clientes

@{
    ViewData["Title"] = "Clientes";
}

@foreach (var cliente in Model.ListaClientes)
{
    <div class="espaco">
        <p><strong>Id:</strong> @cliente.Id</p>
        <p><strong>Nome:</strong> @cliente.Nome</p>
        <p><strong>Fone:</strong>  @cliente.Fone</p>
        <p><strong>Endereco:</strong>  @cliente.Endereco</p>
    </div>
}